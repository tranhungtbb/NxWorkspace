{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/dash-board/ui/statistic-item/src",
  "prefix": "beekin-app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/dash-board/ui/statistic-item"],
      "options": {
        "project": "libs/dash-board/ui/statistic-item/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/dash-board/ui/statistic-item/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/dash-board/ui/statistic-item/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dash-board/ui/statistic-item"],
      "options": {
        "jestConfig": "libs/dash-board/ui/statistic-item/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/dash-board/ui/statistic-item/**/*.ts",
          "libs/dash-board/ui/statistic-item/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
